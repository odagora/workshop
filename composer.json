{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-snappy": "0.4.x",
        "benjamincrozat/laravel-dropbox-driver": "^2.0",
        "doctrine/dbal": "^2.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "jrm2k6/cloudder": "0.7.*",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.4",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "orangehill/iseed": "2.6.4",
        "spatie/laravel-backup": "^6.0.0",
        "spatie/laravel-newsletter": "^4.2.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/test_helpers.php",
            "tests/Mail/MailTracking.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
